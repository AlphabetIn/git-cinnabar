#!/usr/bin/env python2.7

import os
import sys
sys.path.append(os.path.join(os.path.dirname(__file__), 'pythonlib'))

from cinnabar.cmd import CLI
from cinnabar.util import init_logging


def main(argv):
    init_logging()
    return CLI.run(argv)


if __name__ == '__main__':
    sys.exit(main(sys.argv[1:]))
